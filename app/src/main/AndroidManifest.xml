<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Modul5Progmob">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:name="com.example.Modul5Progmob.App"
        android:icon="@mipmap/ic_logo_app_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Modul5Progmob">
        <activity android:name="com.example.Modul5Progmob.DetailKegiatanUserActivity"></activity>
        <activity android:name="com.example.Modul5Progmob.EditEventActivity" />
        <activity android:name="com.example.Modul5Progmob.EditProfileActivity" />
        <activity android:name="com.example.Modul5Progmob.ProfileActivity" />
        <activity android:name="com.example.Modul5Progmob.ShowDetailActivity" />
<!--        <activity android:name=".AccountActivity" />-->
        <activity android:name="com.example.Modul5Progmob.AddEvent" />
        <activity android:name="com.example.Modul5Progmob.HomeActivity" />
        <activity android:name="com.example.Modul5Progmob.RegisterActivity" />
        <activity android:name="com.example.Modul5Progmob.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.Modul5Progmob.MainActivity" />
        <service
            android:name="com.example.Modul5Progmob.service.FcmReceiver"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>